{
  "action": {
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "content_scripts": [
    {
      "css": [
        "contentStyle.css"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "matches": [
        "https://app.inv.tech/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Provides helpers for copying content, viewing diffs, and more.",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 3,
  "name": "MTC Helper",
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ]
    },
    {
      "matches": [
        "https://app.inv.tech/*"
      ],
      "resources": [
        "inject/inject.js"
      ]
    }
  ]
}